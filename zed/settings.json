{
  "agent": {
    "model_parameters": [],
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "Monaspace Neon",
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "ss01": true,
    "ss02": true,
    "ss03": true,
    "ss04": true,
    "ss05": true,
    "ss06": true,
    "ss07": true,
    "ss08": true,
    "ss09": true
  },
  "buffer_font_size": 22,
  "buffer_line_height": "standard",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "icon_theme": "Base Charmed Icons",
  "indent_guides": {
    "enabled": false
  },
  "languages": {
    "PHP": {
      "language_servers": ["intelephense", "!phpactor", "..."]
    }
  },
  "project_panel": {
    "auto_fold_dirs": false,
    "indent_guides": {
      "show": "never"
    },
    "indent_size": 20
  },
  "scrollbar": {
    "axes": {
      "horizontal": false,
      "vertical": false
    }
  },
  "terminal": {
    "cursor_shape": "underline",
    "font_family": "Monaspace Krypton",
    "font_size": 22,
    "line_height": "standard"
  },
  "theme": {
    "dark": "Rosé Pine",
    "light": "Rosé Pine Dawn",
    "mode": "system"
  },
  "ui_font_family": "Atkinson Hyperlegible Next",
  "ui_font_size": 20,
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_system_clipboard": "always"
  },
  "vim_mode": true
}
